        <component name="CP" tempType="LRP">
            <limiter name="CpLowMode"   temp="340:360:410:430" value="-1:2:2:1" />
            <limiter name="SADisable"   temp="410:440" value="-1:1" />
            <limiter name="CpDebugMode" temp="420:460" value="-1:1" />
